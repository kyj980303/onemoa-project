<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bitcamp.onemoaproject.dao.FindChatDao">

    <resultMap id="findChatRoomMap" type="findChat">
        <id column="msgno" property="no"/>
        <result column="mno" property="buyer"/>
        <result column="mno2" property="seller"/>
    </resultMap>

    <select id="findChatRoom" resultMap="findChatRoomMap">
        select
        msgno,
        mno,
        mno2
        from
        message
        where
        mno=1;
    </select>
</mapper>